$color | Select-Oject -Property *
Get-Member -InputObject $color
$color | Get-Member
$color.GetType().Name

$obj = New-Object -TypeName PSObject

//oldway
$obj | Add-Member -MemberType NoteProperty -Name OSBuild -Value 'wadasd'

//new way type accelerator
$myFirstCustomObject = [pscustomobject]@{
    OSBuild = 'OSBuild'
    OSVersion = 'Version'
}


